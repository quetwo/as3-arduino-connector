<?xml version="1.0" encoding="UTF-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/2.5">
	<id>com.quetwo.Arduino.ArduinoConnector</id>
	<name>Arduino Connector</name>
	<copyright>This work is licensed under Mozilla Public License Version 1.1, Copyright 2012</copyright>
	<versionNumber>1.2.0</versionNumber>
	<platforms>
	<platform name="Windows-x86">
		<applicationDeployment>
			<nativeLibrary>libSerialANE.dll</nativeLibrary>
			<initializer>SerialANEinitializer</initializer>
			<finalizer>SerialANEfinalizer</finalizer>
		</applicationDeployment>
	</platform>
	<platform name="MacOS-x86">
		<applicationDeployment>
			<nativeLibrary>SerialANE.framework</nativeLibrary>
			<initializer>SerialANEinitializer</initializer>
			<finalizer>SerialANEfinalizer</finalizer>
		</applicationDeployment>
	</platform>
	</platforms>
</extension>